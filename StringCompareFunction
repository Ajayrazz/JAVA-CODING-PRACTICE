import java.util.*;
public class StringCompareFunction {
    public static void main(String args[]){
        String s1 = "Tony";
        String s2 = "Tony";
        String s3 = new String("Tony");

        //Compare
        if(s1==s2){
            System.out.println("Given string are equal.");
        } else {
            System.out.println("Given string are not equal.");
        }
        if(s1==s3){
            System.out.println("Given string are equal.");
        } else {
            System.out.println("Given string are not equal.");
        }

        // comparing by using (.equals)
        if(s1.equals(s3)){
            System.out.println("Given string are equal.");
        } else {
            System.out.println("Given string are not equal.");
        }
    }
}
