import java.util.*;
public class PRINTING_CHARACTER_PATTERN {
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a value : ");
        int n = sc.nextInt();
        

        //printing character pattern
        for(int i=0; i<n; i++){
            char ch = 'A';
            for(int j=0; j<=i; j++){
                System.out.print(ch);
                ch++;
            }
            System.out.println();
        }
    }
}
