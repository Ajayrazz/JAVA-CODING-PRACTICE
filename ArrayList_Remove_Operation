import java.util.ArrayList;
public class ArrayList_Remove_Operation {
    public static void main(String args[]){
        //className objectName = new className();
        ArrayList<Integer> List = new ArrayList<>();

        //Add Operation
        List.add(4);
        List.add(8);
        List.add(9);
        List.add(2);

        System.out.println(List);

        //Remove Operation
        List.remove(2);
        System.out.println(List);
    }
}
