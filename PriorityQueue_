import java.util.*;
public class PriorityQueue_ {
    public static void main(String args[]){
        PriorityQueue<Integer> pq = new PriorityQueue<>();
        pq.add(5);
        pq.add(8);
        pq.add(1);
        pq.add(0);
        pq.add(4);

        while(!pq.isEmpty()){
            System.out.print(pq.peek());
            pq.remove();
        }
    }
}
